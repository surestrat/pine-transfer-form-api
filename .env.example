# Appwrite Configuration
APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
APPWRITE_PROJECT_ID=your_project_id
APPWRITE_API_KEY=your_api_key
APPWRITE_DATABASE_ID=your_database_id
APPWRITE_COLLECTION_ID=your_collection_id

# CORS Configuration
ALLOWED_ORIGIN=https://yourdomain.com,http://localhost:3000

# Email Configuration (use either the primary or alternative variable names)
# Primary email config variables
SMTP_SERVER=your.smtp.server
SMTP_PORT=465
SMTP_USERNAME=your_email@example.com
SMTP_PASSWORD=your_password
ADMIN_EMAILS=admin1@example.com,admin2@example.com

# Alternative email config variables (used as fallback if primary not found)
SMTP_HOST=your.smtp.server
SMTP_USER=your_email@example.com
SMTP_PASS=your_password
NOTIFICATION_EMAILS=admin1@example.com,admin2@example.com

# External API
EXTERNAL_API_URL=http://gw-test.pineapple.co.za/users/motor_lead
EXTERNAL_API_KEY=your_api_key
EXTERNAL_API_SECRET=your_api_secret

# Server config
PORT=4000
